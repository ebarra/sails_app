<h2>Editar <%= user.nombre %></h2>

<form action="/user/update/<%= user.id %>" method="POST" class="form-signin">
<input type="text" value="<%= user.nombre %>" name="nombre" class="form-control"/>
<input type="text" value="<%= user.sexo %>" name="sexo" class="form-control"/>

<input type="submit" value="Editar" class="btn btn-lg btn-primary"/>
<input type="hidden" name="_csrf" value="<%= _csrf %>"/>
</form>
